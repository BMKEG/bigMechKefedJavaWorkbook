<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
	<name>UimaBioCSentence</name>
	<version>1.0.0</version>
	<vendor></vendor>
	<types>
		<typeDescription>
			<name>bioc.type.UimaBioCSentence</name>
			<description />
			<supertypeName>uima.tcas.Annotation</supertypeName>
			<features>
				<featureDescription>
					<name>offset</name>
					<description>
						A Document offset to where the sentence begins in the
						Passage. This value is the sum of the passage offset and the local
						offset within the passage.
					</description>
					<rangeTypeName>uima.cas.Integer</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>text</name>
					<description>The original text of the sentence.
					</description>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>infons</name>
					<description></description>
					<rangeTypeName>uima.cas.FSArray</rangeTypeName>
					<elementType>bioc.type.MapEntry</elementType>
				</featureDescription>
				<featureDescription>
					<name>annotations</name>
					<description></description>
					<rangeTypeName>uima.cas.FSArray</rangeTypeName>
					<elementType>bioc.type.UimaBioCAnnotation</elementType>
				</featureDescription>
				<featureDescription>
					<name>relations</name>
					<description></description>
					<rangeTypeName>uima.cas.FSArray</rangeTypeName>
					<elementType>bioc.type.UimaBioCRelation</elementType>
				</featureDescription>
			</features>
		</typeDescription>
	</types>
</typeSystemDescription>
